{
  "name": "miojo",
  "version": "0.0.5",
  "description": "miojo is a lightweight functional micro-framework for SPAs. It comes with the essential ingredients — router, state, template engine, and lifecycle hooks — so you can build apps instantly.",
  "license": "MIT",
  "main": "dist/miojo.min.js",
  "types": "dist/index.d.ts",
  "bin": {
    "miojo": "./bin/miojo.js",
    "create-miojo-app": "./bin/create-app.js"
  },
  "files": [
    "dist/",
    "bin/",
    "templates/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npx terser src/index.js -o dist/miojo.min.js -m -c && npm run types",
    "types": "tsc -p tsconfig.types.json --emitDeclarationOnly --declaration --outDir dist || echo \"(skip types)\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "spa",
    "functional",
    "ui",
    "framework",
    "server",
    "cli",
    "javascript",
    "reactive"
  ],
  "author": "Vinícius Barreto",
  "repository": {
    "type": "git",
    "url": "https://github.com/vbfs/miojo.git"
  },
  "bugs": {
    "url": "https://github.com/vbfs/miojo/issues"
  },
  "homepage": "https://github.com/vbfs/miojo#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "preferGlobal": false,
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "terser": "^5.43.1"
  }
}
